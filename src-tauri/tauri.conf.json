{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Cozy Coder",
  "version": "0.1.0",
  "identifier": "app.cozycoder",
  "build": {
    "frontendDist": "http://localhost:4000",
    "devUrl": "http://localhost:4000",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "Cozy Coder",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "deb",
    "externalBin": [
      "../burrito_out/cozycoder"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
